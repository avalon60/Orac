#
# Script to restart ORDS, following database having switched to max_string_size=EXTENDED
#
${HOME}/shutDown.sh immediate
${HOME}/startUp.sh

sqlplus / as sysdba<<EOF
alter pluggable database all open;
select *
from dba_pdbs;
EOF
/var/tmp/dapex/bin/start_ords.sh
